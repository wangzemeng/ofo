/* pages/wallet/wallet.wxss */
.contianer{
  width: 100%;
  height: 100vh;
  background-color: #f2f2f2;
}
.money{
  background-color:#fff;
  padding: 40rpx;
  text-align:center;
}
.money-header{
  width: 100%;
  font-size: 24rpx;
}
.money-content{
  margin:60rpx 0;
  font-size:80rpx;
  font-weight:600;
}
.money-desc text{
  font-size: 24rpx;
  border: 1px solid #ccc;
  padding:10rpx 30rpx;
  border-radius: 40rpx;
}
button{
  background-color: #b9dd08;
  width:90%;
  margin: 40rpx auto;
}
.only{
  display: flex;
  background-color: #fff;
  padding: 40rpx;
  justify-content: space-between;
  font-size: 32rpx;
}
.my-yajin{
  margin-top: 4rpx;
}
.about{
  margin-top: 40rpx;
}